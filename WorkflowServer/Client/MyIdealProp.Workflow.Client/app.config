    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IWorkflowService">
                    <security mode="Message">
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8000/WorkflowService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IWorkflowService"
                contract="WorkflowServer.IWorkflowService" name="NetTcpBinding_IWorkflowService">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
